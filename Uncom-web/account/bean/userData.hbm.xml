<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate?Hibernate Mapping DTD//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="bean.userData" table="user">
   <id name="id" column="id">
    <generator class="uuid2"></generator>
   </id>
    <property name="username" column="username"></property>
    <property name="password" column="password"></property>
    <property name="sex" column="sex"></property>
    <property name="sprovince" column="sprovince"></property>
    <property name="stown" column="stown"></property>
    <property name="sarea" column="sarea"></property>
    <property name="email" column="email"></property>
    <property name="phone" column="phone"></property>
    <property name="headerPic" column="header_pic"></property>
    <property name="province" column="province"></property>
    <property name="town" column="town"></property>
    <property name="area" column="area"></property>
    <property name="createTime" column="create_time"></property>
    <property name="updateTime" column="update_time"></property>
    <property name="qqNumber" column="qq_number"></property>
    <property name="wexinNumber" column="wexin_number	"></property>
    <property name="weibo" column="weibo"></property>	
    <property name="studentId" column="student_id"></property>	
    <property name="studentIdPassword" column="student_id_password"></property>	
    <property name="age" column="age"></property>	
	
    
  </class>

</hibernate-mapping>