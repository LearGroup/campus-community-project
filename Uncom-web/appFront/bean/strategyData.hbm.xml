<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate?Hibernate Mapping DTD//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bean.strategyData" table="strategy">
		<id name="id" column="id">
			<generator class="uuid2"></generator>
		</id>

		<property name="theStrategy" column="the_strategy"></property>
		
		<property name="transport" column="transport"></property>
		<property name="tripAttractionId" column="trip_attraction_id"></property>
		<property name="introduction" column="introduction"></property>
	</class>
</hibernate-mapping>